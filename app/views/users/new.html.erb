<%= form_with model: @users do |form| %>

    <div>
        <%= form.label :name %><br>
        <%= form.text_field :name %>
    </div>

    <div>
        <%= form.label :email %><br>
        <%= form.email_field :email %>
    <div>
    
    <div><br>
        <%= form.submit %>
    </div>
<% end %>

